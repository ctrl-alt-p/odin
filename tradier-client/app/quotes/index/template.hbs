<h1>Quotes list</h1>

<p>
  {{link-to "New Quote" "quotes.new" }}
</p>

{{#if model.length}}
  <table>
    <thead>
      <tr>
        <th>
          Symbol
        </th>
        <th>
          Description
        </th>
        <th>
          Exch
        </th>
        <th>
          Type
        </th>
        <th>
          Change
        </th>
        <th>
          Change percentage
        </th>
        <th>
          Volume
        </th>
        <th>
          Average volume
        </th>
        <th>
          Last volume
        </th>
        <th>
          Trade date
        </th>
        <th>
          Open
        </th>
        <th>
          High
        </th>
        <th>
          Low
        </th>
        <th>
          Close
        </th>
        <th>
          Prevclose
        </th>
        <th>
          Week 52 high
        </th>
        <th>
          Week 52 low
        </th>
        <th>
          Bid
        </th>
        <th>
          Bidsize
        </th>
        <th>
          Bidexch
        </th>
        <th>
          Bid date
        </th>
        <th>
          Ask
        </th>
        <th>
          Asksize
        </th>
        <th>
          Askexch
        </th>
        <th>
          Ask date
        </th>
        <th>
          Open interest
        </th>
        <th>
          Underlying
        </th>
        <th>
          Strike
        </th>
        <th>
          Contract size
        </th>
        <th>
          Expiration date
        </th>
        <th>
          Expiration type
        </th>
        <th>
          Option type
        </th>
        <th colspan="3">
          Actions
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each model as |quote|}}
        <tr>
          <td>
            <a href="/quotes/{{quote.id}}">{{quote.symbol}}</a>
          </td>
          <td>
            {{quote.description}}
          </td>
          <td>
            {{quote.exch}}
          </td>
          <td>
            {{quote.type}}
          </td>
          <td>
            {{quote.change}}
          </td>
          <td>
            {{quote.changePercentage}}
          </td>
          <td>
            {{quote.volume}}
          </td>
          <td>
            {{quote.averageVolume}}
          </td>
          <td>
            {{quote.lastVolume}}
          </td>
          <td>
            {{quote.tradeDate}}
          </td>
          <td>
            {{quote.open}}
          </td>
          <td>
            {{quote.high}}
          </td>
          <td>
            {{quote.low}}
          </td>
          <td>
            {{quote.close}}
          </td>
          <td>
            {{quote.prevclose}}
          </td>
          <td>
            {{quote.week52High}}
          </td>
          <td>
            {{quote.week52Low}}
          </td>
          <td>
            {{quote.bid}}
          </td>
          <td>
            {{quote.bidsize}}
          </td>
          <td>
            {{quote.bidexch}}
          </td>
          <td>
            {{quote.bidDate}}
          </td>
          <td>
            {{quote.ask}}
          </td>
          <td>
            {{quote.asksize}}
          </td>
          <td>
            {{quote.askexch}}
          </td>
          <td>
            {{quote.askDate}}
          </td>
          <td>
            {{quote.openInterest}}
          </td>
          <td>
            {{quote.underlying}}
          </td>
          <td>
            {{quote.strike}}
          </td>
          <td>
            {{quote.contractSize}}
          </td>
          <td>
            {{quote.expirationDate}}
          </td>
          <td>
            {{quote.expirationType}}
          </td>
          <td>
            {{quote.optionType}}
          </td>
          <td>
            {{link-to "Edit" "quotes.edit" quote}}
          </td>
          <td>
            {{link-to "Show" "quotes.show" quote}}
          </td>
          <td>
            <a href="#" {{action "remove" quote}}>Remove</a>
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
{{else}}
  <p id="blankslate">
    No Quotes found
  </p>
{{/if}}
